-- Fly movement loop
RunService.Heartbeat:Connect(function(delta)
    if state.flying then
        local camera = Workspace.CurrentCamera
        
        -- Camera rotation
        if camRot.left then state.flyAng = Vector2.new(state.flyAng.X - 3 * delta, state.flyAng.Y) end
        if camRot.right then state.flyAng = Vector2.new(state.flyAng.X + 3 * delta, state.flyAng.Y) end
        state.flyAng = Vector2.new(state.flyAng.X, math.clamp(state.flyAng.Y, -1.4, 1.4))
        
        -- Movement
        local yaw = state.flyAng.X
        local forward = Vector3.new(math.sin(yaw), 0, math.cos(yaw))
        local right = Vector3.new(math.cos(yaw), 0, -math.sin(yaw))
        
        local move = Vector3.new(0, 0, 0)
        if flyDir.forward then move = move + forward end
        if flyDir.back then move = move - forward end
        if flyDir.right then move = move + right end
        if flyDir.left then move = move - right end
        if flyDir.up then move = move + Vector3.new(0, 1, 0) end
        if flyDir.down then move = move - Vector3.new(0, 1, 0) end
        
        if move.Magnitude > 0 then
            state.flyPos = state.flyPos + move.Unit * state.speed * delta
        end
        
        -- Update camera
        local look = Vector3.new(
            math.sin(yaw) * math.cos(state.flyAng.Y),
            -math.sin(state.flyAng.Y),
            math.cos(yaw) * math.cos(state.flyAng.Y)
        )
        
        camera.CFrame = CFrame.new(state.flyPos, state.flyPos + look)
        camera.FieldOfView = config.cameraFOV
    end
end)

-- ============ TROLL MODE REAL FUNCTIONS ============

-- 1. MAGNET PULL üß≤ - REAL PLAYER ATTRACTION
local function magnetPull()
    statusText.Text = "üß≤ MAGNET PULL ACTIVATED!\nPlayers being pulled to you!"
    
    local char = player.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    local magnetActive = true
    
    -- Create magnetic force
    local connection = RunService.Heartbeat:Connect(function()
        if not magnetActive then return end
        
        for _, target in pairs(Players:GetPlayers()) do
            if target ~= player and target.Character then
                local targetHrp = target.Character:FindFirstChild("HumanoidRootPart")
                if targetHrp then
                    local distance = (targetHrp.Position - hrp.Position).Magnitude
                    if distance < 100 then
                        -- Calculate pull force
                        local direction = (hrp.Position - targetHrp.Position).Unit
                        local force = 100 / math.max(distance, 1)
                        
                        -- Apply velocity directly
                        local bodyVelocity = Instance.new("BodyVelocity")
                        bodyVelocity.Velocity = direction * force
                        bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
                        bodyVelocity.Parent = targetHrp
                        
                        game:GetService("Debris"):AddItem(bodyVelocity, 0.1)
                    end
                end
            end
        end
    end)
    
    task.wait(10)
    magnetActive = false
    connection:Disconnect()
    statusText.Text = "Magnet Pull: Ended"
    task.wait(2)
    updateUI()
end

-- 2. PLAYER CRUSHER üóúÔ∏è - CRUSH PLAYERS TOGETHER
local function playerCrusher()
    statusText.Text = "üóúÔ∏è PLAYER CRUSHER ACTIVATED!\nCrushing players together!"
    
    local playersToCrush = {}
    
    -- Gather all players
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= player and target.Character then
            local hrp = target.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                table.insert(playersToCrush, hrp)
            end
        end
    end
    
    if #playersToCrush < 2 then
        statusText.Text = "Need at least 2 players!"
        task.wait(2)
        updateUI()
        return
    end
    
    -- Calculate center point
    local center = Vector3.new(0, 0, 0)
    for _, hrp in pairs(playersToCrush) do
        center = center + hrp.Position
    end
    center = center / #playersToCrush
    
    -- Crush players to center
    for _, hrp in pairs(playersToCrush) do
        local direction = (center - hrp.Position).Unit
        
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Velocity = direction * 50
        bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
        bodyVelocity.Parent = hrp
        
        game:GetService("Debris"):AddItem(bodyVelocity, 2)
        
        -- Damage on collision
        local humanoid = hrp.Parent:FindFirstChild("Humanoid")
        if humanoid then
            humanoid:TakeDamage(30)
        end
    end
    
    task.wait(2)
    statusText.Text = "Players crushed!"
    task.wait(2)
    updateUI()
end

-- 3. GRAVITY WELL ‚¨áÔ∏è - HEAVY GRAVITY ON PLAYERS
local function gravityWell()
    statusText.Text = "‚¨áÔ∏è GRAVITY WELL ACTIVATED!\nPlayers being pulled down!"
    
    local gravityMultiplier = 5
    local originalGravity = Workspace.Gravity
    
    -- Store original gravity for each player
    local playerGravities = {}
    
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= player and target.Character then
            for _, part in pairs(target.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    -- Store original properties
                    playerGravities[part] = {
                        mass = part.Mass,
                        customPhysicalProperties = part.CustomPhysicalProperties
                    }
                    
                    -- Increase mass dramatically
                    local newProps = PhysicalProperties.new(
                        part.Material,
                        part.Material == Enum.Material.Wood and 0.7 or 0.3,
                        part.Material == Enum.Material.Wood and 0.3 or 0.5,
                        0.5,
                        part.Material == Enum.Material.Wood and 0.8 or 0.2,
                        1
                    )
                    part.CustomPhysicalProperties = newProps
                end
            end
        end
    end
    
    task.wait(5)
    
    -- Restore original properties
    for part, props in pairs(playerGravities) do
        if part.Parent then
            part.CustomPhysicalProperties = props.customPhysicalProperties
        end
    end
    
    statusText.Text = "Gravity Well: Ended"
    task.wait(2)
    updateUI()
end

-- 4. EXPLOSIVE PUNCH üí• - REAL EXPLOSIONS ON PUNCH
local function explosivePunch()
    statusText.Text = "üí• EXPLOSIVE PUNCH ACTIVATED!\nHit players to explode!"
    
    local char = player.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    local punchActive = true
    local lastPunch = 0
    
    local connection = RunService.Heartbeat:Connect(function()
        if not punchActive then return end
        
        for _, target in pairs(Players:GetPlayers()) do
            if target ~= player and target.Character then
                local targetHrp = target.Character:FindFirstChild("HumanoidRootPart")
                if targetHrp then
                    local distance = (targetHrp.Position - hrp.Position).Magnitude
                    if distance < 10 and tick() - lastPunch > 1 then
                        lastPunch = tick()
                        
                        -- Create real explosion
                        local explosion = Instance.new("Explosion")
                        explosion.Position = targetHrp.Position
                        explosion.BlastPressure = 500000
                        explosion.BlastRadius = 15
                        explosion.DestroyJointRadiusPercent = 0.5
                        explosion.ExplosionType = Enum.ExplosionType.CratersAndDebris
                        explosion.Parent = Workspace
                        
                        -- Launch player
                        local bodyVelocity = Instance.new("BodyVelocity")
                        bodyVelocity.Velocity = Vector3.new(
                            math.random(-100, 100),
                            math.random(50, 150),
                            math.random(-100, 100)
                        )
                        bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
                        bodyVelocity.Parent = targetHrp
                        
                        game:GetService("Debris"):AddItem(bodyVelocity, 1)
                        
                        -- Damage
                        local humanoid = target.Character:FindFirstChild("Humanoid")
                        if humanoid then
                            humanoid:TakeDamage(50)
                        end
                    end
                end
            end
        end
    end)
    
    task.wait(15)
    punchActive = false
    connection:Disconnect()
    statusText.Text = "Explosive Punch: Ended"
    task.wait(2)
    updateUI()
end
-- 5. TELEPORT CHAOS üîÄ - RANDOM TELEPORT PLAYERS
local function teleportChaos()
    statusText.Text = "üîÄ TELEPORT CHAOS ACTIVATED!\nPlayers teleporting randomly!"
    
    local chaosActive = true
    
    local connection = RunService.Heartbeat:Connect(function()
        if not chaosActive then return end
        
        for _, target in pairs(Players:GetPlayers()) do
            if target ~= player and target.Character and math.random(1, 10) == 1 then
                local hrp = target.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    hrp.CFrame = CFrame.new(
                        math.random(-500, 500),
                        math.random(10, 100),
                        math.random(-500, 500)
                    )
                end
            end
        end
    end)
    
    task.wait(10)
    chaosActive = false
    connection:Disconnect()
    statusText.Text = "Teleport Chaos: Ended"
    task.wait(2)
    updateUI()
end

-- 6. OBJECT MAGNET üß≤ - PULL ALL OBJECTS TO YOU
local function objectMagnet()
    statusText.Text = "üß≤ OBJECT MAGNET ACTIVATED!\nPulling all objects to you!"
    
    local char = player.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    local magnetActive = true
    local pulledObjects = {}
    
    local connection = RunService.Heartbeat:Connect(function()
        if not magnetActive then return end
        
        -- Pull all unanchored parts
        for _, part in pairs(Workspace:GetDescendants()) do
            if part:IsA("BasePart") and not part.Anchored and part.CanCollide then
                local distance = (part.Position - hrp.Position).Magnitude
                if distance < 200 then
                    local direction = (hrp.Position - part.Position).Unit
                    local force = 50 / math.max(distance, 1)
                    
                    if not pulledObjects[part] then
                        pulledObjects[part] = true
                        
                        local bodyVelocity = Instance.new("BodyVelocity")
                        bodyVelocity.Velocity = direction * force
                        bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
                        bodyVelocity.Parent = part
                        
                        game:GetService("Debris"):AddItem(bodyVelocity, 0.5)
                    end
                end
            end
        end
    end)
    
    task.wait(15)
    magnetActive = false
    connection:Disconnect()
    statusText.Text = "Object Magnet: Ended"
    task.wait(2)
    updateUI()
end

-- 7. PLAYER LAUNCHER üöÄ - LAUNCH PLAYERS TO SKY
local function playerLauncher()
    statusText.Text = "üöÄ PLAYER LAUNCHER ACTIVATED!\nLaunching players to sky!"
    
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= player and target.Character then
            local hrp = target.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                local bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.Velocity = Vector3.new(
                    math.random(-50, 50),
                    math.random(200, 500),
                    math.random(-50, 50)
                )
                bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
                bodyVelocity.Parent = hrp
                
                game:GetService("Debris"):AddItem(bodyVelocity, 3)
            end
        end
    end
    
    task.wait(3)
    statusText.Text = "Players launched!"
    task.wait(2)
    updateUI()
end

-- 8. SLOW MOTION ‚è±Ô∏è - SLOW DOWN EVERYTHING
local function slowMotion()
    statusText.Text = "‚è±Ô∏è SLOW MOTION ACTIVATED!"
    
    local originalGravity = Workspace.Gravity
    Workspace.Gravity = originalGravity * 0.3
    
    -- Slow down all players
    local playerSpeeds = {}
    
    for _, target in pairs(Players:GetPlayers()) do
        if target.Character then
            local humanoid = target.Character:FindFirstChild("Humanoid")
            if humanoid then
                playerSpeeds[target] = humanoid.WalkSpeed
                humanoid.WalkSpeed = humanoid.WalkSpeed * 0.3
            end
        end
    end
    
    task.wait(10)
    
    -- Restore
    Workspace.Gravity = originalGravity
    
    for target, speed in pairs(playerSpeeds) do
        if target.Character then
            local humanoid = target.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = speed
            end
        end
    end
    
    statusText.Text = "Slow Motion: Ended"
    task.wait(2)
    updateUI()
end

-- 9. INVISIBLE MODE üëª - REAL INVISIBILITY
local function invisibleMode()
    statusText.Text = "üëª INVISIBLE MODE ACTIVATED!"
    
    local char = player.Character
    if not char then return end
    
    -- Make truly invisible
    for _, part in pairs(char:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Transparency = 1
            part.CastShadow = false
        end
    end
    
    -- Remove humanoid display
    local humanoid = char:FindFirstChild("Humanoid")
    if humanoid then
        humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
    end
    
    -- Remove nametag
    for _, child in pairs(char:GetChildren()) do
        if child:IsA("BillboardGui") then
            child.Enabled = false
        end
    end
    
    task.wait(15)
    
    -- Restore
    for _, part in pairs(char:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Transparency = 0
            part.CastShadow = true
        end
    end
    
    if humanoid then
        humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.Viewer
    end
    
    for _, child in pairs(char:GetChildren()) do
        if child:IsA("BillboardGui") then
            child.Enabled = true
        end
    end
    
    statusText.Text = "Invisible Mode: Ended"
    task.wait(2)
    updateUI()
end

-- 10. SUPER STRENGTH üí™ - MOVE HEAVY OBJECTS
local function superStrength()
    statusText.Text = "üí™ SUPER STRENGTH ACTIVATED!\nCan move any object!"
    
    local char = player.Character
    if not char then return end
    
    -- Make player stronger
    local humanoid = char:FindFirstChild("Humanoid")
    if humanoid then
        humanoid.JumpPower = 200
        humanoid.WalkSpeed = 100
    end
    
    -- Can move anchored objects
    local mouse = player:GetMouse()
    
    mouse.Button1Down:Connect(function()
        local target = mouse.Target
        if target and target:IsA("BasePart") then
            if target.Anchored then
                target.Anchored = false
                
                -- Apply force
                local bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.Velocity = mouse.Hit.LookVector * 100
                bodyVelocity.MaxForce = Vector3.new(100000, 100000, 100000)
                bodyVelocity.Parent = target
                
                game:GetService("Debris"):AddItem(bodyVelocity, 0.5)
            end
        end
    end)
    
    task.wait(20)
    
    if humanoid then
        humanoid.JumpPower = 50
        humanoid.WalkSpeed = 16
    end
    
    statusText.Text = "Super Strength: Ended"
    task.wait(2)
    updateUI()
end

-- 11. SPEED STEAL üèÉ‚Äç‚ôÇÔ∏è - STEAL SPEED FROM PLAYERS
local function speedSteal()
    statusText.Text = "üèÉ‚Äç‚ôÇÔ∏è SPEED STEAL ACTIVATED!\nStealing speed from players!"
    
    local char = player.Character
    if not char then return end
    
    local humanoid = char:FindFirstChild("Humanoid")
    if not humanoid then return end
    
    local originalSpeed = humanoid.WalkSpeed
    local stealActive = true
    local stolenSpeeds = {}
    
    local connection = RunService.Heartbeat:Connect(function()
        if not stealActive then return end
        
        local totalStolenSpeed = 0
        
        for _, target in pairs(Players:GetPlayers()) do
            if target ~= player and target.Character then
                local targetHumanoid = target.Character:FindFirstChild("Humanoid")
                if targetHumanoid then
                    -- Store original if not already
                    if not stolenSpeeds[target] then
                        stolenSpeeds[target] = targetHumanoid.WalkSpeed
                    end
                    
                    -- Reduce their speed
                    targetHumanoid.WalkSpeed = math.max(5, targetHumanoid.WalkSpeed - 2)
                    
                    -- Add to your speed
                    totalStolenSpeed = totalStolenSpeed + 2
                end
            end
        end
        
        -- Increase your speed
        humanoid.WalkSpeed = originalSpeed + totalStolenSpeed
    end)
    
    task.wait(10)
    stealActive = false
    connection:Disconnect()
    
    -- Restore stolen speeds
    for target, speed in pairs(stolenSpeeds) do
        if target.Character then
            local targetHumanoid = target.Character:FindFirstChild("Humanoid")
            if targetHumanoid then
                targetHumanoid.WalkSpeed = speed
            end
        end
    end
    
    humanoid.WalkSpeed = originalSpeed
    statusText.Text = "Speed Steal: Ended"
    task.wait(2)
    updateUI()
end

-- 12. PLAYER FREEZE ‚ùÑÔ∏è - FREEZE PLAYERS IN PLACE
local function playerFreeze()
    statusText.Text = "‚ùÑÔ∏è PLAYER FREEZE ACTIVATED!\nFreezing all players!"
    
    local frozenPlayers = {}
    
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= player and target.Character then
            local char = target.Character
            
            -- Store original properties
            frozenPlayers[target] = {}
            
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA("BasePart") then
                    table.insert(frozenPlayers[target], {
                        part = part,
                        anchored = part.Anchored,
                        velocity = part.Velocity
                    })
                    
                    -- Freeze in place
                    part.Anchored = true
                    part.Velocity = Vector3.new(0, 0, 0)
                end
            end
        end
    end
    
    task.wait(10)
    
    -- Restore
    for target, partsData in pairs(frozenPlayers) do
        if target.Character then
            for _, data in pairs(partsData) do
                if data.part.Parent then
                    data.part.Anchored = data.anchored
                    data.part.Velocity = data.velocity
                end
            end
        end
    end
    
    statusText.Text = "Player Freeze: Ended"
    task.wait(2)
    updateUI()
end

-- 13. MASS TELEPORT üå™Ô∏è - TELEPORT ALL PLAYERS TO ONE SPOT
local function massTeleportReal()
    statusText.Text = "üå™Ô∏è MASS TELEPORT ACTIVATED!\nTeleporting all players!"
    
    local teleportPos = player.Character.HumanoidRootPart.Position + Vector3.new(0, 10, 0)
    
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= player and target.Character then
            local hrp = target.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                hrp.CFrame = CFrame.new(teleportPos)
            end
        end
    end
    
    task.wait(2)
    statusText.Text = "All players teleported!"
    task.wait(2)
    updateUI()
end

-- 14. FORCE PUSH üëä - PUSH PLAYERS AWAY
local function forcePush()
    statusText.Text = "üëä FORCE PUSH ACTIVATED!\nPushing players away!"
    
    local char = player.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= player and target.Character then
            local targetHrp = target.Character:FindFirstChild("HumanoidRootPart")
            if targetHrp then
                local distance = (targetHrp.Position - hrp.Position).Magnitude
                if distance < 50 then
                    local direction = (targetHrp.Position - hrp.Position).Unit
                    local force = 200 * (1 - distance/50)
                    
                    local bodyVelocity = Instance.new("BodyVelocity")
                    bodyVelocity.Velocity = direction * force
                    bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
                    bodyVelocity.Parent = targetHrp
                    
                    game:GetService("Debris"):AddItem(bodyVelocity, 1)
                end
            end
        end
    end
    
    task.wait(2)
    statusText.Text = "Force Push: Complete"
    task.wait(2)
    updateUI()
end
-- 15. PLAYER ATTRACT üß≤ - ATTRACT PLAYERS TO YOU
local function playerAttract()
    statusText.Text = "üß≤ PLAYER ATTRACT ACTIVATED!\nPlayers attracted to you!"
    
    local char = player.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    local attractActive = true
    
    local connection = RunService.Heartbeat:Connect(function()
        if not attractActive then return end
        
        for _, target in pairs(Players:GetPlayers()) do
            if target ~= player and target.Character then
                local targetHrp = target.Character:FindFirstChild("HumanoidRootPart")
                if targetHrp then
                    local direction = (hrp.Position - targetHrp.Position).Unit
                    local distance = (targetHrp.Position - hrp.Position).Magnitude
                    
                    if distance < 100 then
                        local force = 30 * (1 - distance/100)
                        
                        local bodyVelocity = Instance.new("BodyVelocity")
                        bodyVelocity.Velocity = direction * force
                        bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
                        bodyVelocity.Parent = targetHrp
                        
                        game:GetService("Debris"):AddItem(bodyVelocity, 0.1)
                    end
                end
            end
        end
    end)
    
    task.wait(15)
    attractActive = false
    connection:Disconnect()
    statusText.Text = "Player Attract: Ended"
    task.wait(2)
    updateUI()
end

-- 16. OBJECT RAIN ‚òî - MAKE OBJECTS FALL FROM SKY
local function objectRainReal()
    statusText.Text = "‚òî OBJECT RAIN ACTIVATED!\nObjects falling from sky!"
    
    local objects = {}
    
    for i = 1, 50 do
        local part = Instance.new("Part")
        part.Size = Vector3.new(
            math.random(2, 10),
            math.random(2, 10),
            math.random(2, 10)
        )
        part.Position = Vector3.new(
            math.random(-50, 50),
            math.random(100, 200),
            math.random(-50, 50)
        )
        part.BrickColor = BrickColor.Random()
        part.Material = Enum.Material.Plastic
        part.Parent = Workspace
        
        -- Make it fall fast
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Velocity = Vector3.new(0, -100, 0)
        bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
        bodyVelocity.Parent = part
        
        table.insert(objects, {part = part, velocity = bodyVelocity})
        task.wait(0.1)
    end
    
    task.wait(5)
    
    for _, data in pairs(objects) do
        data.part:Destroy()
    end
    
    statusText.Text = "Object Rain: Ended"
    task.wait(2)
    updateUI()
end

-- 17. ANTI-GRAVITY üåå - REVERSE GRAVITY FOR PLAYERS
local function antiGravityReal()
    statusText.Text = "üåå ANTI-GRAVITY ACTIVATED!\nPlayers floating up!"
    
    local playerBodyForces = {}
    
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= player and target.Character then
            local hrp = target.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                local bodyForce = Instance.new("BodyForce")
                bodyForce.Force = Vector3.new(0, 196.2 * hrp:GetMass(), 0)
                bodyForce.Parent = hrp
                
                playerBodyForces[target] = bodyForce
            end
        end
    end
    
    task.wait(10)
    
    for target, bodyForce in pairs(playerBodyForces) do
        if bodyForce then
            bodyForce:Destroy()
        end
    end
    
    statusText.Text = "Anti-Gravity: Ended"
    task.wait(2)
    updateUI()
end

-- 18. PLAYER SPIN üåÄ - SPIN PLAYERS IN CIRCLES
local function playerSpin()
    statusText.Text = "üåÄ PLAYER SPIN ACTIVATED!\nSpinning players!"
    
    local char = player.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    local spinActive = true
    local spinTime = 0
    
    local connection = RunService.Heartbeat:Connect(function(delta)
        if not spinActive then return end
        
        spinTime = spinTime + delta
        
        for _, target in pairs(Players:GetPlayers()) do
            if target ~= player and target.Character then
                local targetHrp = target.Character:FindFirstChild("HumanoidRootPart")
                if targetHrp then
                    local distance = (targetHrp.Position - hrp.Position).Magnitude
                    if distance < 30 then
                        -- Calculate spin position
                        local angle = spinTime * 5
                        local radius = 10
                        local offset = Vector3.new(
                            math.cos(angle) * radius,
                            0,
                            math.sin(angle) * radius
                        )
                        
                        targetHrp.CFrame = CFrame.new(hrp.Position + offset)
                    end
                end
            end
        end
    end)
    
    task.wait(10)
    spinActive = false
    connection:Disconnect()
    statusText.Text = "Player Spin: Ended"
    task.wait(2)
    updateUI()
end

-- 19. SUPER JUMP ü¶ò - EXTREME JUMP FOR EVERYONE
local function superJumpReal()
    statusText.Text = "ü¶ò SUPER JUMP ACTIVATED!\nEveryone jumping high!"
    
    local playerJumps = {}
    
    for _, target in pairs(Players:GetPlayers()) do
        if target.Character then
            local humanoid = target.Character:FindFirstChild("Humanoid")
            if humanoid then
                playerJumps[target] = humanoid.JumpPower
                humanoid.JumpPower = 500
                
                -- Make them jump
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end
    end
    
    task.wait(5)
    
    -- Restore jump power
    for target, jumpPower in pairs(playerJumps) do
        if target.Character then
            local humanoid = target.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.JumpPower = jumpPower
            end
        end
    end
    
    statusText.Text = "Super Jump: Ended"
    task.wait(2)
    updateUI()
end

-- 20. EARTHQUAKE üåã - SHAKE EVERYTHING
local function earthquake()
    statusText.Text = "üåã EARTHQUAKE ACTIVATED!\nShaking everything!"
    
    local shakeActive = true
    local shakeTime = 0
    
    local connection = RunService.Heartbeat:Connect(function(delta)
        if not shakeActive then return end
        
        shakeTime = shakeTime + delta
        
        -- Shake all unanchored parts
        for _, part in pairs(Workspace:GetDescendants()) do
            if part:IsA("BasePart") and not part.Anchored then
                local shakeAmount = math.sin(shakeTime * 10) * 10
                part.CFrame = part.CFrame * CFrame.new(
                    math.random(-shakeAmount, shakeAmount) * 0.1,
                    math.random(-shakeAmount, shakeAmount) * 0.1,
                    math.random(-shakeAmount, shakeAmount) * 0.1
                )
            end
        end
        
        -- Shake players
        for _, target in pairs(Players:GetPlayers()) do
            if target ~= player and target.Character then
                local hrp = target.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    local shakeAmount = math.sin(shakeTime * 15) * 5
                    hrp.CFrame = hrp.CFrame * CFrame.new(
                        math.random(-shakeAmount, shakeAmount) * 0.2,
                        0,
                        math.random(-shakeAmount, shakeAmount) * 0.2
                    )
                end
            end
        end
    end)
    
    task.wait(10)
    shakeActive = false
    connection:Disconnect()
    statusText.Text = "Earthquake: Ended"
    task.wait(2)
    updateUI()
end
