-- Functions
local function updateCounter()
    if gui and gui:FindFirstChild("MainPanel") then
        local counter = gui.MainPanel:FindFirstChild("Counter")
        if counter then
            counter.Text = "ðŸ“ MARKS: " .. #marks .. "/" .. MAX_MARKS
        end
    end
end

local function createMark(position, lookVector)
    if #marks >= MAX_MARKS then
        warn("[Camera Cheat] Max marks reached!")
        return nil
    end
    
    local mark = {
        position = position,
        lookVector = lookVector,
        part = nil
    }
    
    -- Create visual marker
    local part = Instance.new("Part")
    part.Name = "CheatMark_" .. #marks + 1
    part.Size = Vector3.new(2, 2, 2)
    part.Position = position
    part.Anchored = true
    part.CanCollide = false
    part.Material = EnumMaterial.Neon
    part.Color = Color3.fromRGB(0, 255, 255)
    part.Transparency = 0.3
    
    -- Add beam to mark
    local beam = Instance.new("Beam")
    beam.Color = ColorSequence.new(Color3.fromRGB(0, 255, 255))
    beam.Width0 = 0.5
    beam.Width1 = 0.5
    beam.Attachment0 = Instance.new("Attachment")
    beam.Attachment0.Parent = part
    beam.Attachment0.Position = Vector3.new(0, 1, 0)
    beam.Attachment1 = Instance.new("Attachment")
    beam.Attachment1.Parent = part
    beam.Attachment1.Position = Vector3.new(0, 5, 0)
    beam.Parent = part
    
    part.Parent = workspace
    mark.part = part
    
    table.insert(marks, mark)
    updateCounter()
    
    print("[Camera Cheat] Mark placed at " .. tostring(position))
    return mark
end

local function clearAllMarks()
    for _, mark in ipairs(marks) do
        if mark.part and mark.part.Parent then
            mark.part:Destroy()
        end
    end
    marks = {}
    currentMark = 0
    updateCounter()
    
    if gui and gui:FindFirstChild("ViewPanel") then
        gui.ViewPanel.ViewInfo.Text = "NO MARKS PLACED"
        gui.ViewPanel.NavFrame.PrevBtn.Visible = false
        gui.ViewPanel.NavFrame.NextBtn.Visible = false
        gui.ViewPanel.NavFrame.ExitBtn.Visible = true
    end
end

local function viewMark(index)
    if #marks == 0 or index < 1 or index > #marks then return end
    
    currentMark = index
    local mark = marks[index]
    
    camera.CameraType = Enum.CameraType.Scriptable
    camera.CFrame = CFrame.new(mark.position, mark.position + mark.lookVector)
    
    -- Update UI
    if gui and gui:FindFirstChild("ViewPanel") then
        local panel = gui.ViewPanel
        panel.ViewInfo.Text = "MARK " .. index .. "/" .. #marks
        
        panel.NavFrame.PrevBtn.Visible = (index > 1)
        panel.NavFrame.NextBtn.Visible = (index < #marks)
        panel.NavFrame.ExitBtn.Visible = true
    end
end

local function toggleFreeCamera()
    isFreeCam = not isFreeCam
    
    if isFreeCam then
        -- Enable free camera
        camera.CameraType = Enum.CameraType.Scriptable
        
        -- Save original camera position
        if not camera:FindFirstChild("OriginalCFrame") then
            local value = Instance.new("CFrameValue")
            value.Name = "OriginalCFrame"
            value.Value = camera.CFrame
            value.Parent = camera
        end
        
        -- Freeze player
        if player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.PlatformStand = true
            end
        end
        
        -- Update button
        if gui then
            gui.MainPanel.ToggleCam.Text = "EXIT CAM"
            gui.MainPanel.ToggleCam.BackgroundColor3 = Color3.fromRGB(220, 70, 70)
        end
        
        print("[Camera Cheat] Free camera enabled")
    else
        -- Disable free camera
        camera.CameraType = Enum.CameraType.Custom
        
        -- Unfreeze player
        if player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.PlatformStand = false
            end
        end
        
        -- Update button
        if gui then
            gui.MainPanel.ToggleCam.Text = "FREE CAM"
            gui.MainPanel.ToggleCam.BackgroundColor3 = Color3.fromRGB(70, 130, 200)
        end
        
        print("[Camera Cheat] Free camera disabled")
    end
end

local function toggleViewMode()
    if gui then
        local viewPanel = gui.ViewPanel
        viewPanel.Visible = not viewPanel.Visible
        
        if viewPanel.Visible and #marks > 0 then
            viewMark(1)
        else
            camera.CameraType = isFreeCam and Enum.CameraType.Scriptable or Enum.CameraType.Custom
        end
    end
end

-- Setup GUI and Events
gui = createCheatGUI()

-- Button hover effects
local function setupButton(button)
    local originalColor = button.BackgroundColor3
    
    button.MouseButton1Down:Connect(function()
        button.BackgroundColor3 = originalColor * 0.7
    end)
    
    button.MouseButton1Up:Connect(function()
        button.BackgroundColor3 = originalColor
    end)
    
    button.MouseEnter:Connect(function()
        if not UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1) then
            button.BackgroundColor3 = originalColor * 1.1
        end
    end)
    
    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = originalColor
    end)
end

-- Apply effects to all buttons
for _, button in pairs(gui:GetDescendants()) do
    if button:IsA("TextButton") then
        setupButton(button)
    end
end

-- Button click events
gui.MainPanel.ToggleCam.MouseButton1Click:Connect(toggleFreeCamera)

gui.MainPanel.PlaceMark.MouseButton1Click:Connect(function()
    if isFreeCam then
        createMark(camera.CFrame.Position, camera.CFrame.LookVector)
    else
        warn("[Camera Cheat] Enable free camera first!")
    end
end)

gui.MainPanel.ViewMarks.MouseButton1Click:Connect(toggleViewMode)

gui.MainPanel.ClearMarks.MouseButton1Click:Connect(clearAllMarks)

gui.MainPanel.SpeedControl.SpeedUp.MouseButton1Click:Connect(function()
    CAM_SPEED = math.min(CAM_SPEED + 5, MAX_SPEED)
    gui.MainPanel.SpeedControl.SpeedLabel.Text = "SPEED: " .. CAM_SPEED
end)

gui.MainPanel.SpeedControl.SpeedDown.MouseButton1Click:Connect(function()
    CAM_SPEED = math.max(CAM_SPEED - 5, MIN_SPEED)
    gui.MainPanel.SpeedControl.SpeedLabel.Text = "SPEED: " .. CAM_SPEED
end)

-- View panel navigation
gui.ViewPanel.NavFrame.PrevBtn.MouseButton1Click:Connect(function()
    if currentMark > 1 then
        viewMark(currentMark - 1)
    end
end)

gui.ViewPanel.NavFrame.NextBtn.MouseButton1Click:Connect(function()
    if currentMark < #marks then
        viewMark(currentMark + 1)
    end
end)

gui.ViewPanel.NavFrame.ExitBtn.MouseButton1Click:Connect(toggleViewMode)

-- Hotkeys
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.F then
        toggleFreeCamera()
    elseif input.KeyCode == Enum.KeyCode.M and isFreeCam then
        createMark(camera.CFrame.Position, camera.CFrame.LookVector)
    elseif input.KeyCode == Enum.KeyCode.V then
        toggleViewMode()
    elseif input.KeyCode == Enum.KeyCode.C then
        clearAllMarks()
    elseif input.KeyCode == Enum.KeyCode.LeftBracket then
        CAM_SPEED = math.max(CAM_SPEED - 5, MIN_SPEED)
        if gui then gui.MainPanel.SpeedControl.SpeedLabel.Text = "SPEED: " .. CAM_SPEED end
    elseif input.KeyCode == Enum.KeyCode.RightBracket then
        CAM_SPEED = math.min(CAM_SPEED + 5, MAX_SPEED)
        if gui then gui.MainPanel.SpeedControl.SpeedLabel.Text = "SPEED: " .. CAM_SPEED end
    end
end)

-- Mouse wheel for speed
mouse.WheelForward:Connect(function()
    if isFreeCam then
        CAM_SPEED = math.min(CAM_SPEED + 5, MAX_SPEED)
        if gui then gui.MainPanel.SpeedControl.SpeedLabel.Text = "SPEED: " .. CAM_SPEED end
    end
end)

mouse.WheelBackward:Connect(function()
    if isFreeCam then
        CAM_SPEED = math.max(CAM_SPEED - 5, MIN_SPEED)
        if gui then gui.MainPanel.SpeedControl.SpeedLabel.Text = "SPEED: " .. CAM_SPEED end
    end
end)

-- Free camera movement
local cameraInput = {
    W = false,
    A = false,
    S = false,
    D = false,
    Q = false,
    E = false
}

UserInputService.InputBegan:Connect(function(input)
    if not isFreeCam then return end
    
    if input.KeyCode == Enum.KeyCode.W then cameraInput.W = true
    elseif input.KeyCode == Enum.KeyCode.A then cameraInput.A = true
    elseif input.KeyCode == Enum.KeyCode.S then cameraInput.S = true
    elseif input.KeyCode == Enum.KeyCode.D then cameraInput.D = true
    elseif input.KeyCode == Enum.KeyCode.Q then cameraInput.Q = true
    elseif input.KeyCode == Enum.KeyCode.E then cameraInput.E = true
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if not isFreeCam then return end
    
    if input.KeyCode == Enum.KeyCode.W then cameraInput.W = false
    elseif input.KeyCode == Enum.KeyCode.A then cameraInput.A = false
    elseif input.KeyCode == Enum.KeyCode.S then cameraInput.S = false
    elseif input.KeyCode == Enum.KeyCode.D then cameraInput.D = false
    elseif input.KeyCode == Enum.KeyCode.Q then cameraInput.Q = false
    elseif input.KeyCode == Enum.KeyCode.E then cameraInput.E = false
    end
end)

-- Camera movement loop
RunService.RenderStepped:Connect(function(deltaTime)
    if isFreeCam then
        local moveVector = Vector3.new(0, 0, 0)
        
        if cameraInput.W then moveVector = moveVector + camera.CFrame.LookVector end
        if cameraInput.S then moveVector = moveVector - camera.CFrame.LookVector end
        if cameraInput.D then moveVector = moveVector + camera.CFrame.RightVector end
        if cameraInput.A then moveVector = moveVector - camera.CFrame.RightVector end
        if cameraInput.E then moveVector = moveVector + Vector3.new(0, 1, 0) end
        if cameraInput.Q then moveVector = moveVector - Vector3.new(0, 1, 0) end
        
        if moveVector.Magnitude > 0 then
            camera.CFrame = camera.CFrame + (moveVector.Unit * CAM_SPEED * deltaTime)
        end
        
        -- Mouse look
        if UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2) then
            local delta = UserInputService:GetMouseDelta()
            local yaw = delta.X * 0.003
            local pitch = delta.Y * 0.003
            
            camera.CFrame = camera.CFrame * CFrame.fromOrientation(-pitch, -yaw, 0)
        end
    end
end)

-- Notification
local function notify(msg)
    print("[Camera Cheat] " .. msg)
    
    -- Optional: Create notification
    local notif = Instance.new("ScreenGui")
    notif.Name = "Notification_" .. math.random(1000, 9999)
    notif.Parent = game.CoreGui
    
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 300, 0, 60)
    frame.Position = UDim2.new(0.5, -150, 0.1, 0)
    frame.BackgroundColor3 = Color3.fromRGB(30, 30, 50)
    frame.BorderSizePixel = 0
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = frame
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = "ðŸ“· " .. msg
    label.TextColor3 = C
    label.Font = Enum.Font.GothamBold
    label.TextSize = 14
    label.Parent = frame
    
    frame.Parent = notif
    
    task.wait(3)
    notif:Destroy()
end

notify("Camera Cheat v3.0 loaded! by sherikyt")
notify("Hotkeys: F=Free Cam, M=Mark, V=View, C=Clear")
notify("Mouse wheel = Speed, RMB = Rotate")

print("=== Camera Cheat v3.0 ===")
print("Creator: sherikyt")
print("Features:")
print("- Free Camera Movement (WASD + QE)")
print("- Place Marks (10 max)")
print("- View Marks with Navigation")
print("- Adjustable Speed Control")
print("==========================")
